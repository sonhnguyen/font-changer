{
  "manifest_version": 2,
  "name": "Font-Weight Changer",
  "description": "This extension will change all webpages' font-weight to 500 and disable stickies",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Change font-weight"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}